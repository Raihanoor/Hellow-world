#include <bits/stdc++.h>

using namespace std;
int main(){
    int t,q=0,c=0;
    string s;
    cin >> t;
    getchar();
    for(int T=0;T<t;T++){
        getline(cin,s);
       // cout << s.length();
        c=0;
        for (int i = 0; i < s.length() ; ++i) {
            if(s[i]=='a'||s[i]=='d'||s[i]=='g'||s[i]=='j'||s[i]=='m'||s[i]=='p'||s[i]=='t'||s[i]=='w'||s[i]==' ') c++;
            else if(s[i]=='b'||s[i]=='e'||s[i]=='h'||s[i]=='k'||s[i]=='n'||s[i]=='q'||s[i]=='u'||s[i]=='x') c+=2;
            else if(s[i]=='c'||s[i]=='f'||s[i]=='i'||s[i]=='l'||s[i]=='o'||s[i]=='r'||s[i]=='v'||s[i]=='y'||s[i]) c+=3;
            else if(s[i]=='s'||s[i]=='z')
            c+=4;
            //cout << c << endl;
        }
        printf("Case #%d: %d\n",++q,c);
    }


    return 0;
}
