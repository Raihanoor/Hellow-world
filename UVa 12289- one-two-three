#include <bits/stdc++.h>

using namespace std;

int main() {
    int t;
    cin >> t;
    getchar();
    while(t--){
        char num[5];
        cin >> num;
        if(strlen(num)==5){
            cout << "3" << endl;
        }
        else if(num[0]=='o'&&num[1]=='n'||num[0]=='o'&&num[2]=='e'||num[2]=='e'&&num[1]=='n')
        {
            cout << "1" << endl;
        } else cout << "2" << endl;
    }

    return 0;
}
