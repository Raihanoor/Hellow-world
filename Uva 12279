#include <bits/stdc++.h>

using namespace std;


int main() {
    int n,s,p = 1;

    while (cin >> n && n) {
        int c = 0;
        p++;
        while (n != 0) {
            n--;
            cin >> s;
            if (s > 0) {
                c++;
            } else {
                c--;
            }
        }
        cout << "Case " << p-1 << ": " << c << endl;

    }
    return 0;

}
